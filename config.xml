<?xml version="1.0"?>
<configdata>
    <production>
        <siteurl>http://uploadbin.net/</siteurl>
        <sitename>UploadBin</sitename>
	<admin>
	  <email>info@uploadbin.net</email>
	  <name>Uploadbin.net</name>
	</admin>
	<debug>0</debug>
        <uploaddir>/home/olle/uploadbin_uploads/</uploaddir>
        <hashtype>uuid</hashtype>
		<cookielifetime>2592000</cookielifetime>
        <akismetkey></akismetkey>
	    <spamcheck>0</spamcheck>
        <denyextensions></denyextensions>
        <maxexecutiontime>3600</maxexecutiontime>
        <maxuploadsize>256M</maxuploadsize>
        <maxdescinlist>50</maxdescinlist>
        <viruscheckers>VirusTotalHash,ClamScan,ComodoHash</viruscheckers>
        <database>
            <type>pdo_mysql</type>
            <host>localhost</host>
            <username></username>
            <password></password>
            <name></name>
            <prefix></prefix>
            <table>
            	<files>files</files>
            	<formkeys>formkeys</formkeys>
            </table>
        </database>
    </production>
    <staging extends="production">
        <siteurl>http://localhost/easyfup/</siteurl>
        <uploaddir>./uploads/</uploaddir>
	<debug>1</debug>
        <database>
            <host>localhost</host>
            <username></username>
            <password></password>
            <name></name>
        </database>
    </staging>
</configdata>
